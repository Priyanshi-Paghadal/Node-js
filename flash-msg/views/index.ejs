<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <% if (messages.length> 0) { %>
        <ul>
            <% messages.forEach(function(message) { %>
                <li>
                    <%= message %>
                </li>
                <% }); %>
        </ul>
        <% } %>
        <a href="/flash">Flash Msg</a>
</body>

</html>